import java.util.*;
import java.io.*;

public class Lesson47Histogram {

	public static void main(String[] args) {
		Scanner sf = new Scanner(new File("Data2.txt"));
	       int maxIndx = -1;
	       String text[] = new String[1000]; // To be safe, declare more than we need
	       while (sf.hasNext()) {
	           maxIndx++;
	           text[maxIndx] = sf.nextLine();
	           
	       }

	}

}
